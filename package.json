{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:mac": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:test": "eslint \"{src,apps,libs,test}/**/*.spec.ts\" --fix",
    "generate-route": "node ./utils/generate.route.js",
    "generate-tests": "node ./utils/generate.tests.js",
    "generate-tests:lint": "node ./utils/generate.tests.js && npm run lint:test",
    "test": "SET NODE_ENV=development && jest",
    "test:mac": "NODE_ENV=development jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:auth": "SET NODE_ENV=development && jest src/auth/auth.service.spec.ts",
    "test:user": "SET NODE_ENV=development && jest src/user/user.service.spec.ts",
    "test:cron": "SET NODE_ENV=development && jest src/cron/cron.service.spec.ts",
    "test:player": "SET NODE_ENV=development && jest ./src/player/player.service.spec.ts",
    "test:player-details": "SET NODE_ENV=development && jest ./src/player-details/player-details.service.spec.ts",
    "test:realdata": "SET NODE_ENV=development && jest src/realdata/realdata.service.spec.ts",
    "test:sync": "SET NODE_ENV=development && jest src/sync/sync.service.spec.ts",
    "test:team": "SET NODE_ENV=development && jest ./src/team/team.service.spec.ts",
    "test:user-settings": "SET NODE_ENV=development && jest src/user-settings/user.settings.service.spec.ts",
    "test:one-on-one": "SET NODE_ENV=development && jest ./src/records/one-on-one/one-on-one.service.spec.ts",
    "test:random": "SET NODE_ENV=development && jest ./src/records/random/random.service.spec.ts",
    "test:stopwatch-shootout": "SET NODE_ENV=development && jest ./src/records/stopwatch-shootout/stopwatch-shootout.service.spec.ts",
    "test:three-points-contest": "SET NODE_ENV=development && jest ./src/records/three-points-contest/three-points-contest.service.spec.ts",
    "test:tournament": "SET NODE_ENV=development && jest ./src/records/tournament/tournament.service.spec.ts",
    "test:api": "SET NODE_ENV=development && jest ./src/sync/api",
    "test:records": "SET NODE_ENV=development && jest ./src/records",
    "test:services": "SET NODE_ENV=development && jest ./src/auth ./src/cron ./src/player ./src/player-details ./src/realdata ./src/sync/sync.service.spec.ts ./src/team ./src/user-settings ./src/records"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:mac"
    }
  },
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/schedule": "^0.4.2",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "cors-anywhere": "^0.4.4",
    "fs": "0.0.1-security",
    "fs-extra": "^9.1.0",
    "handlebars": "^4.7.7",
    "inquirer": "^6.0.0",
    "lint-staged": "^10.5.4",
    "nba": "^4.13.0",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/express": "^4.17.8",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.37",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "link-module-alias": "^1.2.0",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "12.20.0"
  }
}
